.logo-cont
    .logo
        position relative
        height fit-content
        width fit-content
        flex-cont(row, center, flex-end)

        .background
            height 100px
            width 100px
            background Grey_light_1
            border-radius 50%
            border 1.04px solid #F5F5F5
            box-shadow 4.16667px 6.25px 16.6667px rgba(0, 0, 0, 0.12), -4.16667px -4.16667px 8.33333px rgba(255, 255, 255, 0.9), inset 0px 4px 0px #fdfdfd, inset 0px -4px 0px #f0f0f0
            position relative
            left 50%
            transform translate(-50%, 0%)

        img
            position relative
            left calc(50% + -102px)
            transform translate(calc(-50% - 9.5px), -4px)

    .mail, .logo
        .check
            flex-cont(row, center, center)
            height 62px
            width 62px
            background blue
            border-radius 50%
            box-shadow 0px 4.32468px 14.4156px rgba(0, 0, 0, 0.15), inset 0px 4px 0px rgba(#73F2FF, 0.2), inset 0px -2px 0px #00C2E2

