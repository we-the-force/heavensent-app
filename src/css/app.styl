@import 'stylus/constants/*'
@import 'stylus/components/*'
@import 'stylus/pages/*'

.page
    .page-content
        padding 15px
        position absolute
        width 100%
        top 0

        .list ul
            background none

            &:before, &:after
                content none

        p.text
            font-template(normal, 14px, 24px)
            text-align center