{
  "cwd": "/Users/gamanox-wtf/wtf/heaven-sent-app",
  "type": [
    "web",
    "pwa",
    "cordova"
  ],
  "name": "HeavenSent",
  "framework": "core",
  "template": "split-view",
  "bundler": "webpack",
  "cssPreProcessor": "stylus",
  "theming": {
    "customColor": true,
    "color": "#007aff",
    "darkTheme": false,
    "iconFonts": true,
    "fillBars": false
